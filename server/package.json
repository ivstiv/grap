{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -r dotenv-flow/config --require ./dist/tests/utils.js ./dist/tests",
    "coverage": "nyc --reporter=json-summary --reporter=text pnpm run test",
    "coverage-badge": "make-coverage-badge",
    "build": "rm -rf dist && tsc -p tsconfig.json && cp -r src/public dist",
    "start": "node --trace-warnings -r dotenv-flow/config dist/index.js",
    "dev-watch": "tsc-watch --noClear -p tsconfig.json --onSuccess 'node dist/index.js'",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "lint:watch": "esw --quiet --watch --fix --ext .ts",
    "knex": "node -r dotenv-flow/config ./node_modules/knex/bin/cli.js --knexfile ./dist/database/knexfile.js",
    "migrate:up": "pnpm run knex migrate:up",
    "migrate:down": "pnpm run knex migrate:down",
    "migrate:rollback": "pnpm run knex migrate:rollback",
    "migrate:latest": "pnpm run knex migrate:latest",
    "migrate:make": "node ./node_modules/knex/bin/cli.js --migrations-directory src/database/migrations migrate:make -x ts",
    "seed": "pnpm run knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/session": "^10.2.0",
    "@fastify/static": "^6.10.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.7.0",
    "@fastify/view": "^7.4.1",
    "@types/lodash": "^4.14.192",
    "@types/mocha": "^10.0.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "fastify": "^4.15.0",
    "fastify-type-provider-zod": "^1.1.9",
    "knex": "^2.4.2",
    "liquidjs": "^10.7.0",
    "lodash": "^4.17.21",
    "mailparser": "^3.6.4",
    "objection": "^3.0.1",
    "smtp-server": "^3.11.0",
    "sqlite3": "^5.1.6",
    "unique-names-generator": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-flow": "^3.2.0",
    "@types/mailparser": "^3.4.0",
    "@types/node": "^18.15.11",
    "@types/smtp-server": "^3.5.7",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "make-coverage-badge": "^1.2.0",
    "mocha": "^10.2.0",
    "mocha-steps": "^1.3.0",
    "node-html-parser": "^6.1.5",
    "nyc": "^15.1.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^5.0.4"
  }
}
