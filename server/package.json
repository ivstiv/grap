{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha -r dotenv-flow/config --require ./dist/tests/utils.js ./dist/tests",
    "coverage": "nyc --reporter=json-summary --reporter=text pnpm run test",
    "coverage-badge": "make-coverage-badge",
    "build": "rm -rf dist && tsc -p tsconfig.json && cp -r src/public dist",
    "start": "node --trace-warnings -r dotenv-flow/config dist/index.js",
    "dev-watch": "tsc-watch --noClear -p tsconfig.json --onSuccess 'node dist/index.js'",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "lint:watch": "esw --quiet --watch --fix --ext .ts",
    "knex": "node -r dotenv-flow/config ./node_modules/knex/bin/cli.js --knexfile ./dist/database/knexfile.js",
    "migrate:up": "pnpm run knex migrate:up",
    "migrate:down": "pnpm run knex migrate:down",
    "migrate:rollback": "pnpm run knex migrate:rollback",
    "migrate:latest": "pnpm run knex migrate:latest",
    "migrate:make": "node ./node_modules/knex/bin/cli.js --migrations-directory src/database/migrations migrate:make -x ts",
    "seed": "pnpm run knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "eslint",
        "@typescript-eslint/eslint-plugin",
        "@typescript-eslint/parser"
      ]
    }
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/session": "^10.9.0",
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "@fastify/view": "^9.1.0",
    "@types/lodash": "^4.17.6",
    "@types/mocha": "^10.0.7",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^2.0.0",
    "knex": "^3.1.0",
    "liquidjs": "^10.14.0",
    "lodash": "^4.17.21",
    "mailparser": "^3.7.1",
    "objection": "^3.1.4",
    "smtp-server": "^3.13.4",
    "sqlite3": "^5.1.7",
    "unique-names-generator": "^4.7.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv-flow": "^3.3.3",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.14.10",
    "@types/smtp-server": "^3.5.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "make-coverage-badge": "^1.2.0",
    "mocha": "^10.6.0",
    "mocha-steps": "^1.3.0",
    "node-html-parser": "^6.1.13",
    "nyc": "^17.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.3"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
