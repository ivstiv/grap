{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "nodemon --watch src -e ts --exec 'npm run lint:fix && npm run build && npm run start'",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "lint:watch": "esw --quiet --watch --fix --ext .ts",
    "knex": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile ./dist/database/knexfile.js",
    "migrate:up": "npm run knex migrate:up",
    "migrate:down": "npm run knex migrate:down",
    "migrate:rollback": "npm run knex migrate:rollback",
    "migrate:latest": "npm run knex migrate:latest",
    "migrate:make": "node ./node_modules/knex/bin/cli.js --migrations-directory src/database/migrations migrate:make -x ts",
    "seed": "npm run knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^7.0.0",
    "@fastify/formbody": "^7.0.1",
    "@fastify/session": "^9.0.0",
    "@fastify/view": "^7.0.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "fastify": "^4.0.0",
    "knex": "^2.1.0",
    "mailparser": "^3.5.0",
    "objection": "^3.0.1",
    "smtp-server": "^3.11.0",
    "sqlite3": "^5.0.8",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.1",
    "@types/mailparser": "^3.4.0",
    "@types/node": "^17.0.41",
    "@types/smtp-server": "^3.5.7",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.7.3"
  }
}
